{
    "name": "nodegui-simple-starter",
    "version": "1.0.0",
    "description": "A NodeGui starter project",
    "scripts": {
        "dev": "node esbuild.config.cjs && qode dist/main.cjs",
        "build": "node esbuild.config.cjs",
        "package": "jam-pack-nodegui --config packaging/jam-pack-nodegui.json",
        "run": "qode dist/main.cjs"
    },
    "devDependencies": {
        "@types/node": "^20.3.1",
        "@types/source-map-support": "^0.5.10",
        "esbuild": "^0.18.20",
        "jam-pack-nodegui": "^0.18.0",
        "typescript": "^5.1.3"
    },
    "dependencies": {
        "@nodegui/nodegui": "^0.61.0",
        "esbuild-plugin-inline-image": "^0.0.9",
        "open-explorer-cross-platform": "^1.0.3",
        "source-map-support": "^0.5.21"
    }
}
