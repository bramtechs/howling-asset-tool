{
    "name": "nodegui-simple-starter",
    "version": "1.0.0",
    "description": "A NodeGui starter project",
    "scripts": {
        "dev": "node esbuild.config.mjs && qode dist/main.cjs",
        "build": "node esbuild.config.mjs",
        "package": "jam-pack-nodegui --config packaging/jam-pack-nodegui.json",
        "format": "prettier --write .",
        "lint": "eslint --config eslint.config.mjs",
        "lint:fix": "eslint --config eslint.config.mjs --fix",
        "tidy": "npm run format && npm run lint:fix",
        "test": "echo \"no tests yet\"",
        "run": "qode dist/main.cjs"
    },
    "devDependencies": {
        "@eslint/js": "^9.15.0",
        "@jest/globals": "^29.7.0",
        "@types/ini": "^4.1.1",
        "@types/node": "^20.3.1",
        "@types/source-map-support": "^0.5.10",
        "@typescript-eslint/eslint-plugin": "^8.15.0",
        "@typescript-eslint/parser": "^8.15.0",
        "esbuild": "^0.18.20",
        "esbuild-plugin-inline-image": "^0.0.9",
        "eslint": "^9.15.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.12.0",
        "jam-pack-nodegui": "^0.18.0",
        "prettier": "^3.3.3",
        "typescript": "^5.1.3",
        "typescript-eslint": "^8.15.0"
    },
    "dependencies": {
        "@nodegui/nodegui": "^0.61.0",
        "ini": "^5.0.0",
        "open-explorer-cross-platform": "^1.0.3",
        "source-map-support": "^0.5.21"
    }
}
